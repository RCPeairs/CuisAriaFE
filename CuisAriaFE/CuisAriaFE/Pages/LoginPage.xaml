<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CuisAriaFE.Pages.LoginPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="CuisAria" Font="Arial"/>
            <Entry x:Name="userNameEntry" Placeholder="Enter User Name Here"/>
            <Button Text="Login" Clicked="OnLoginClicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>